{
    "name": "cec",
    "version": "1.0.0",
    "description": "CEC_AEGIS_ISHIMURA",
    "main": "IshimuraDB.js",
    "private": true,
    "scripts": {
        "start": "npm-run-all --parallel server crew",
        "RIG": "nodemon CEC/archive/ships/USG_Ishimura/IshimuraDB.js",
        "server": "nodemon CEC/archive/ships/USG_Ishimura/bridge/server/server.js",
        "crew": "webpack serve --config webpack.config.js --mode development",
        "deck": "concurrently \"npm run server\" \"npm run crew\"",
        "build": "webpack --config webpack.config.js --mode production",
        "compile": "npm-run-all --parallel compile-ts compile-scss",
        "compile-ts": "tsc --project tsconfig.json --outDir CEC/archive",
        "compile-scss": "sass CEC/styles/styles.scss CEC/styles/styles.css",
        "test": "jest",
        "lint": "eslint '**/*.{js,jsx,ts,tsx}' --fix",
        "lint:check": "eslint '**/*.{js,jsx,ts,tsx}'",
        "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,html,css,scss,json,md}'",
        "format": "npm run prettier && npm run lint"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ChewBaccaYeti/DS_API.git"
    },
    "keywords": [
        "CEC",
        "Aegis",
        "Ishimura"
    ],
    "author": "Venedykt Mitishov (Isaac CLarke)",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ChewBaccaYeti/DS_API/issues"
    },
    "homepage": "https://github.com/ChewBaccaYeti/DS_API#readme",
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dompurify": "^3.1.7",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "helmet": "^8.0.0",
        "mongodb": "^6.9.0",
        "mongoose": "^8.7.1",
        "morgan": "^1.10.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.27.0",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "@babel/cli": "^7.25.9",
        "@babel/core": "^7.26.0",
        "@babel/preset-env": "^7.26.0",
        "@babel/preset-react": "^7.25.9",
        "@babel/preset-typescript": "^7.26.0",
        "@eslint/js": "^9.14.0",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/cors": "^2.8.17",
        "@types/dotenv": "^8.2.0",
        "@types/eslint-config-prettier": "^6.11.3",
        "@types/express": "^5.0.0",
        "@types/node": "^22.8.2",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^8.13.0",
        "@typescript-eslint/parser": "^8.13.0",
        "babel-loader": "^9.2.1",
        "concurrently": "^9.1.0",
        "css-loader": "^7.1.2",
        "eslint": "^9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.37.2",
        "html-webpack-plugin": "^5.6.3",
        "jest": "^29.7.0",
        "mini-css-extract-plugin": "^2.9.1",
        "nodemon": "^3.1.7",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.3.3",
        "react-scripts": "latest",
        "sass": "^1.80.5",
        "sass-loader": "^16.0.2",
        "style-loader": "^4.0.0",
        "ts-node": "^10.9.2",
        "typescript": "^4.9.5",
        "webpack": "^5.95.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.1.0"
    }
}
